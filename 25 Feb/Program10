import java.util.Arrays;
import java.util.stream.Stream;
import java.util.*;

class Main {
    public static void main(String[] args) {
        String[] names = {"James", "Aryan", "Vibha", "Aniket"};
        Stream<String> list = Arrays.stream(names)
                                    .map(str -> String.valueOf(str.charAt(0))); 
                                    
        list.forEach(System.out::println);  
    }
}





// Program to extract First Letter of Each Word from String Array
